<p id="notice"><%= notice %></p>

<div id="voiced-questions-top" class="questions-top">
  <% if not (@questions.nil? or @questions.empty?) %>
    <%= render 'questions/all_questions' %>
  <% else %>
    <h2>No Voiced Questions</h2>
  <% end %>
</div>

<nav class="navbar navbar-default navbar-static-bottom" role="navigation">
  <%= will_paginate @questions %>
</nav>